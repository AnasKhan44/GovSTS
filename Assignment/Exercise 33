const num_array:number[]=[1,2,3,4,5,6,7,8,9]
function Ordinal(numberArray: number[]) {
    for (let i = 0; i < numberArray.length; i++) {
    const num = numberArray[i];
    let suffix: string;
    if (num === 1) {
    suffix = "st";
    } else if (num === 2) {
        suffix = "nd";
    } else if (num === 3) {
        suffix = "rd";
    } else { suffix = "th"; 
    console.log(`${num}${suffix}`);
    }
}
Ordinal(num_array);
